{
	"useTabs": false,
	"singleQuote": true,
	"printWidth": 80,
	"proseWrap": "always",
	"tabWidth": 4,
	"trailingComma": "none",
	"bracketSpacing": true,
	"semi": true,
	"plugins": ["prettier-plugin-svelte"],
	"pluginSearchDirs": ["."],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
